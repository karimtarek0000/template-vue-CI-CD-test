{
  "name": "for-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:optimized",
    "build:optimized": "npm run clean && npm run build:ssg && npm run critical:extract && npm run critical:analyze && npm run critical:report",
    "build:ssg": "vite-ssg build",
    "build:critical": "node scripts/build-critical.js",
    "critical:extract": "node scripts/critical-css.js",
    "critical:analyze": "node scripts/analyze-critical.js",
    "critical:report": "echo 'âœ¨ Critical CSS optimization complete! Run npm run preview to test your optimized build.'",
    "clean": "rm -rf dist",
    "preview": "vite preview",
    "preview:ssg": "vite-ssg preview",
    "test": "vitest",
    "test:changed": "vitest --changed=origin/master",
    "test:cov": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,vue,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,vue,ts,tsx,json,css,scss,md}\""
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.12",
    "@vueuse/core": "^13.0.0",
    "@vueuse/head": "^2.0.0",
    "lodash-es": "^4.17.21",
    "vite-plugin-bundle-prefetch": "^0.0.4",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/lodash": "^4.17.16",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "3.1.2",
    "autoprefixer": "^10.4.21",
    "cheerio": "^1.0.0",
    "critical": "^7.2.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^10.0.0",
    "fs-extra": "^11.3.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "vite": "^6.2.0",
    "vite-ssg": "^27.0.1",
    "vitest": "^3.1.2",
    "vue-eslint-parser": "^10.1.3"
  }
}
